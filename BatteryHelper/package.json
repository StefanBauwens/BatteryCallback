{
  "name": "BatteryHelper",
  "author": "Stefan Bauwens",
  "version": "1.0.0",
  "keywords": ["pebble-app"],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "Battery Helper",
    "uuid": "bfa0936b-a7ab-433c-8092-a93e934b9944",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite"
    ],
    "capabilities": [
      "configurable"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "sendAtFixedTime",
      "sendWhenBatteryChanged",
      "sendWhenAppOpened",
      "fixedTime",
      "endpoint",
      "JSReady",
      "battery",
      "permissionToCloseApp",
      "httpError"
    ],
    "resources": {
      "media": [
        {
          "type": "png",
          "name": "APP_ICON",
          "file": "images/app_icon.png"
        }
      ]
    }
  },
  "icon": "APP_ICON"
}
